import datetime #para  poder traer  la fecha del dia de hoy

nombre = "jose"
edad = 30

#fecha_actual = datetime.date.today() ## papra extraer la fecha  del dia de hoy
fecha_actual = datetime.date.today().year ## para extraer el año
calculo_de_edad = fecha_actual - edad 
print(calculo_de_edad)
